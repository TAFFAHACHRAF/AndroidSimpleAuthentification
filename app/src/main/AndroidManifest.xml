        </activity>
        <activity android:name=".SignUpActivity"></activity>
        <activity android:name=".GetStartedActivity"></activity>
        <activity android:name=".LoginActivity"></activity>
        <activity android:name=".UserActivity"></activity>
    </application>

</manifest>